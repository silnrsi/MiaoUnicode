# GSUB


lookup replaceFinalsWithYcmp {

	sub @clsFina by @clsFinaYcmp;

} replaceFinalsWithYcmp;

lookup replaceTonesWithBlank {

	sub @clsPosMark by @clsPos;

} replaceTonesWithBlank;


feature clig {

	sub @clsFina'lookup replaceFinalsWithYcmp @clsFina'lookup replaceFinalsWithYcmp @clsFina'lookup replaceFinalsWithYcmp @clsTonesAboveOrBelow;
	sub @clsFina'lookup replaceFinalsWithYcmp @clsFina'lookup replaceFinalsWithYcmp @clsTonesAboveOrBelow;
	sub @clsFina'lookup replaceFinalsWithYcmp @clsTonesAboveOrBelow;

	sub @clsFina @clsPosMark'lookup replaceTonesWithBlank;
	sub @clsFinaYcmp @clsPosMark'lookup replaceTonesWithBlank;

} clig;


# GPOS


lookup shiftFinalToneRightPosition {

	# /* kern right finals (without aspiration mark) */
	# clsFina {kern.x =    0m} / clsInitMidNoKern  _ OPT_FINA_SEQ gRight;
	pos @clsInitMidNoKern @clsFina' <0 0 0 0> @clsFina @clsFina miao_tone_right.blnk;
	pos @clsInitMidNoKern @clsFina' <0 0 0 0> @clsFina miao_tone_right.blnk;
	pos @clsInitMidNoKern @clsFina' <0 0 0 0> miao_tone_right.blnk;

	# clsFina {kern.x =  -80m} / clsInitMidMinKern _ OPT_FINA_SEQ gRight;
	pos @clsInitMidMinKern @clsFina' <-80 0 0 0> @clsFina @clsFina miao_tone_right.blnk;
	pos @clsInitMidMinKern @clsFina' <-80 0 0 0> @clsFina miao_tone_right.blnk;
	pos @clsInitMidMinKern @clsFina' <-80 0 0 0> miao_tone_right.blnk;

	# clsFina {kern.x = -130m} / clsInitMidMajKern _ OPT_FINA_SEQ gRight;
	pos @clsInitMidMajKern @clsFina' <-130 0 0 0> @clsFina @clsFina miao_tone_right.blnk;
	pos @clsInitMidMajKern @clsFina' <-130 0 0 0> @clsFina miao_tone_right.blnk;
	pos @clsInitMidMajKern @clsFina' <-130 0 0 0> miao_tone_right.blnk;

	# clsFina {kern.x =  -40m} / clsAsp _ OPT_FINA_SEQ gRight;
	pos @clsAsp @clsFina' <-40 0 0 0> @clsFina @clsFina miao_tone_right.blnk;
	pos @clsAsp @clsFina' <130 0 0 0> @clsFina miao_tone_right.blnk; # value changed
#	pos @clsAsp @clsFina' <-40 0 0 0> @clsFina miao_tone_right.blnk; # original
	pos @clsAsp @clsFina' <130 0 0 0> miao_tone_right.blnk; # value changed
#	pos @clsAsp @clsFina' <-40 0 0 0> miao_tone_right.blnk; # original

} shiftFinalToneRightPosition;

lookup shiftFinalYToneRightPosition {

	# clsFinaStdYShift {shift.y = 219m} / _ OPT_FINA_SEQ gRight ;
	pos @clsFinaStdYShift' <0 219 0 0> @clsFina @clsFina miao_tone_right.blnk;
	pos @clsFinaStdYShift' <0 219 0 0> @clsFina miao_tone_right.blnk;
	pos @clsFinaStdYShift' <0 219 0 0> miao_tone_right.blnk;

	# gA {shift.y = 219m+80m}  / _ OPT_FINA_SEQ gRight ;
	pos miao_a' <0 299 0 0> @clsFina @clsFina miao_tone_right.blnk;
	pos miao_a' <0 299 0 0> @clsFina miao_tone_right.blnk;
	pos miao_a' <0 299 0 0> miao_tone_right.blnk;

} shiftFinalYToneRightPosition;

lookup shiftFinalToneTopRightPosition {

	# TODO later, clsAspLo -> miao_ref_asp not used by BFM or Lipo
	# /* kern topright finals with respect to Lo initial kerning class for combinations like V,æŽ³ (values inverted) */
	# clsFina {kern.x = -90m} / clsInitLoMinKern  clsAspLo _ OPT_FINA_SEQ gTopRight;
	# clsFina {kern.x = -60m} / clsInitLoMajKern  clsAspLo _ OPT_FINA_SEQ gTopRight;
	# clsFina {kern.x =  40m} / clsInitLoChuxKern clsAspLo _ OPT_FINA_SEQ gTopRight;

	# /* kern topright finals with respect to Hi initial kerning class */
	# clsFina {kern.x =    0m} / clsInitHiNoKern  _ OPT_FINA_SEQ gTopRight;
	pos @clsInitHiNoKern @clsFina' <0 0 0 0> @clsFina @clsFina miao_tone_top_right.blnk;
	pos @clsInitHiNoKern @clsFina' <0 0 0 0> @clsFina miao_tone_top_right.blnk;
	pos @clsInitHiNoKern @clsFina' <0 0 0 0> miao_tone_top_right.blnk;

	# clsFina {kern.x =  -90m} / clsInitHiMinKern _ OPT_FINA_SEQ gTopRight;
	pos @clsInitHiMinKern @clsFina' <-90 0 0 0> @clsFina @clsFina miao_tone_top_right.blnk;
	pos @clsInitHiMinKern @clsFina' <-90 0 0 0> @clsFina miao_tone_top_right.blnk;
	pos @clsInitHiMinKern @clsFina' <-90 0 0 0> miao_tone_top_right.blnk;

	# clsFina {kern.x = -160m} / clsInitHiMajKern _ OPT_FINA_SEQ gTopRight;
	pos @clsInitHiMajKern @clsAspHi @clsFina' <60 0 0 0> @clsFina @clsFina miao_tone_top_right.blnk;
	pos @clsInitHiMajKern @clsAspHi @clsFina' <60 0 0 0> @clsFina miao_tone_top_right.blnk;
	pos @clsInitHiMajKern @clsAspHi @clsFina' <60 0 0 0> miao_tone_top_right.blnk;

	pos @clsInitHiMajKern @clsFina' <-160 0 0 0> @clsFina @clsFina miao_tone_top_right.blnk;
	pos @clsInitHiMajKern @clsFina' <-160 0 0 0> @clsFina miao_tone_top_right.blnk;
	pos @clsInitHiMajKern @clsFina' <-160 0 0 0> miao_tone_top_right.blnk;

	# clsFina {kern.x =   10m} / clsAspHi _ OPT_FINA_SEQ gTopRight;
#	pos @clsAspHi @clsFina' <10 0 0 0> @clsFina miao_tone_top_right.blnk; #original - OK for Android 4.4 but not 7
	pos @clsAspHi @clsFina' <180 0 0 0> @clsFina miao_tone_top_right.blnk; #value changed
#	pos @clsAspHi @clsFina' <10 0 0 0> miao_tone_top_right.blnk; #original - OK for Android 4.4 but not 7
	pos @clsAspHi @clsFina' <190 0 0 0> miao_tone_top_right.blnk; #value changed

	# TODO later, clsAspLo -> miao_ref_asp not used by BFM or Lipo
	# clsFina {kern.x = -190m} / clsInitHiNoKern  clsAspLo _ OPT_FINA_SEQ gTopRight;
	# clsFina {kern.x = -240m} / clsInitHiMinKern clsAspLo _ OPT_FINA_SEQ gTopRight;
	# clsFina {kern.x = -340m} / clsInitHiMajKern clsAspLo _ OPT_FINA_SEQ gTopRight;
	
} shiftFinalToneTopRightPosition;

lookup shiftFinalYToneTopRightPosition {

	# /* shift final(s) up to "tone top right" position (438m, 2/3 of cap height) */
	# clsFinaStdYShift {shift.y = 438m} / _ OPT_FINA_SEQ gTopRight ;
	pos @clsFinaStdYShift' <0 438 0 0> @clsFina @clsFina miao_tone_top_right.blnk;
	pos @clsFinaStdYShift' <0 438 0 0> @clsFina miao_tone_top_right.blnk;
	pos @clsFinaStdYShift' <0 438 0 0> miao_tone_top_right.blnk;

	# gA {shift.y = 438m+160m} / _ OPT_FINA_SEQ gTopRight ;
	pos miao_a' <0 598 0 0> @clsFina @clsFina miao_tone_top_right.blnk;
	pos miao_a' <0 598 0 0> @clsFina miao_tone_top_right.blnk;
	pos miao_a' <0 598 0 0> miao_tone_top_right.blnk;

} shiftFinalYToneTopRightPosition;

lookup kernBaselineFinals {

	# /* kern baseline finals with respect to Hi initial kerning class for combinations like A'o (values inverted) */
	#  clsFina {kern.x =  -90m} / clsInitHiMinKern clsAspHi _ ;
	ignore pos @clsInitHiMinKern @clsAspHi @clsFina' @clsFina @clsFina @clsPos;
	ignore pos @clsInitHiMinKern @clsAspHi @clsFina' @clsFina @clsPos;
	ignore pos @clsInitHiMinKern @clsAspHi @clsFina' @clsPos;
	pos @clsInitHiMinKern @clsAspHi @clsFina' <-90 0 0 0>;

	#  clsFina {kern.x =  -20m} / clsInitHiMajKern clsAspHi _ ;
	ignore pos @clsInitHiMajKern @clsAspHi @clsFina' @clsFina @clsFina @clsPos;
	ignore pos @clsInitHiMajKern @clsAspHi @clsFina' @clsFina @clsPos;
	ignore pos @clsInitHiMajKern @clsAspHi @clsFina' @clsPos;
	pos @clsInitHiMajKern @clsAspHi @clsFina' <-20 0 0 0>;

	# /* kern baseline finals with respect to Lo initial kerning class */
	#  clsFina {kern.x =  -60m} / clsInitLoMinKern _ ;
	ignore pos @clsInitLoMinKern @clsFina' @clsFina @clsFina @clsPos;
	ignore pos @clsInitLoMinKern @clsFina' @clsFina @clsPos;
	ignore pos @clsInitLoMinKern @clsFina' @clsPos;
	pos @clsInitLoMinKern @clsFina' <-60 0 0 0>;

	#  clsFina {kern.x = -160m} / clsInitLoMajKern _ ;
	ignore pos @clsInitLoMajKern @clsFina' @clsFina @clsFina @clsPos;
	ignore pos @clsInitLoMajKern @clsFina' @clsFina @clsPos;
	ignore pos @clsInitLoMajKern @clsFina' @clsPos;
	pos @clsInitLoMajKern @clsFina' <-160 0 0 0>;

	#  clsFina {kern.x = -320m} / clsInitLoChuxKern _ ;
	ignore pos @clsInitLoChuxKern @clsFina' @clsFina @clsFina @clsPos;
	ignore pos @clsInitLoChuxKern @clsFina' @clsFina @clsPos;
	ignore pos @clsInitLoChuxKern @clsFina' @clsPos;
	pos @clsInitLoChuxKern @clsFina' <-320 0 0 0>;

	#  clsFina {kern.x = -190m} / clsInitLoNoKern  clsAspHi _ ;
	ignore pos @clsInitLoNoKern @clsAspHi @clsFina' @clsFina @clsFina @clsPos;
	ignore pos @clsInitLoNoKern @clsAspHi @clsFina' @clsFina @clsPos;
	ignore pos @clsInitLoNoKern @clsAspHi @clsFina' @clsPos;
#	pos @clsInitLoNoKern @clsAspHi @clsFina' <-190 0 0 0>; #original
	pos @clsInitLoNoKern @clsAspHi @clsFina' <-50 0 0 0>; #value changed

	#  clsFina {kern.x = -220m} / clsInitLoMinKern clsAspHi _ ;
	ignore pos @clsInitLoMinKern @clsAspHi @clsFina' @clsFina @clsFina @clsPos;
	ignore pos @clsInitLoMinKern @clsAspHi @clsFina' @clsFina @clsPos;
	ignore pos @clsInitLoMinKern @clsAspHi @clsFina' @clsPos;
#	pos @clsInitLoMinKern @clsAspHi @clsFina' <-220 0 0 0>; #original
#	pos @clsInitLoMinKern @clsAspHi @clsFina' <-50 0 0 0>; #value changed, may not be needed

	#  clsFina {kern.x = -300m} / clsInitLoMajKern clsAspHi _ ;
	ignore pos @clsInitLoMajKern @clsAspHi @clsFina' @clsFina @clsPos;
	ignore pos @clsInitLoMajKern @clsAspHi @clsFina' @clsPos;
#	pos @clsInitLoMajKern @clsAspHi @clsFina' <-300 0 0 0>; #original
	pos @clsInitLoMajKern @clsAspHi @clsFina' <-100 0 0 0>; #value changed

	#  clsFina {kern.x = -480m} / clsInitLoChuxKern clsAspHi _ ;
	ignore pos @clsInitLoChuxKern @clsAspHi @clsFina' @clsFina @clsFina @clsPos;
	ignore pos @clsInitLoChuxKern @clsAspHi @clsFina' @clsFina @clsPos;
	ignore pos @clsInitLoChuxKern @clsAspHi @clsFina' @clsPos;
	pos @clsInitLoChuxKern @clsAspHi @clsFina' <-480 0 0 0>;

} kernBaselineFinals;

lookup addRSB {

	pos @clsInit' <0 0 750 0> @clsAspHi'<-750 0 0 0> @clsFina'<-750 0 0 0> @clsFina @clsPosRight;

	pos @clsInit' <0 0 600 0> @clsAspHi'<-600 0 0 0> @clsFina400_500'<-600 0 0 0> @clsPosRight;
	pos @clsInit' <0 0 550 0> @clsAspHi'<-550 0 0 0> @clsFina300_400'<-550 0 0 0> @clsPosRight;
	pos @clsInit' <0 0 450 0> @clsAspHi'<-450 0 0 0> @clsFina200_300'<-450 0 0 0> @clsPosRight;
	pos @clsInit' <0 0 200 0> @clsAspHi @clsFinaYcmp miao_tone_above.blnk;

	pos @clsInit' <0 0 650 0> @clsAspHi @clsFina200_300'<-650 0 0 0> @clsFina;

	pos @clsInit' <0 0 650 0> @clsFina' <-650 0 0 0> @clsFina;

	pos @clsInit' <0 0 400 0> @clsFina400_500' <-400 0 0 0> @clsPosRight;
	pos @clsInit' <0 0 300 0> @clsFina300_400' <-300 0 0 0> @clsPosRight;
	pos @clsInit' <0 0 200 0> @clsFina200_300' <-200 0 0 0> @clsPosRight;

	pos @clsInit' <0 0 250 0> @clsAspHi @clsFina' <-250 0 0 0>;

	pos @clsInit' <0 0 300 0> @clsFina' <-300 0 0 0>;

} addRSB;

lookup attachAspMark {

	pos @clsInit @clsFinaYcmp @clsPos @clsAspHi'lookup at_aspHiS_with_aspHiM;
	pos @clsInit @clsAspHi'lookup at_aspHiS_with_aspHiM;
	pos @clsInit @clsAspLo'lookup at_aspLoS_with_aspLoM;

} attachAspMark;

lookup finalsAbove {

	# 3 finals on top
	pos @clsInit @clsFinaYcmp'lookup at_aboveNormS3_with_NormL @clsFinaYcmp @clsFinaYcmp miao_tone_above.blnk;

	# 2 finals on top
	pos @clsInit @clsAsp @clsFinaYcmp'lookup at_aboveNormS_with_NormL @clsFinaYcmp miao_tone_above.blnk;
	pos @clsInit @clsFinaYcmp'lookup at_aboveNormS_with_NormL @clsFinaYcmp miao_tone_above.blnk;

	# 1 final on top
	pos @clsInit @clsAsp @clsFinaYcmp'lookup at_aboveNormS_with_NormM miao_tone_above.blnk;
	pos @clsInit @clsFinaYcmp'lookup at_aboveNormS_with_NormM miao_tone_above.blnk;

} finalsAbove;

lookup finalsBelow {
	# TODO later, bottom position not used by BFM or Lipo
	pos @clsInit @clsFinaYcmp'lookup at_belowNormS_with_NormM miao_tone_below.blnk;

} finalsBelow;


feature mark {

	lookup attachAspMark;
	lookup finalsAbove;
	lookup finalsBelow;

} mark;

feature mkmk {

	pos @clsFina'lookup regularSizedFinals;
	pos @clsFinaYcmp'lookup at_NormL_with_NormR;

} mkmk;

lookup shift_miao_a {

	pos @clsFina @clsFina miao_a' <0 160 0 0> [miao_tone_top_right.blnk];
	pos @clsFina miao_a' <0 160 0 0> [miao_tone_top_right.blnk];

	pos @clsFina @clsFina miao_a' <0 80 0 0> [miao_tone_right.blnk];
	pos @clsFina miao_a' <0 80 0 0> [miao_tone_right.blnk];

} shift_miao_a;

feature kern {

	pos miao_asp' <-150 0 0 0> @clsInit;

	lookup shiftFinalToneRightPosition;
	lookup shiftFinalYToneRightPosition;

	lookup shiftFinalToneTopRightPosition;
	lookup shiftFinalYToneTopRightPosition;

	lookup kernBaselineFinals;
	lookup addRSB;

	lookup shift_miao_a;

} kern;


